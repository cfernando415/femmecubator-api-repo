{
  "name": "femmecubator-api-repo",
  "version": "1.0.0",
  "description": "Node.js backend for Femmecubator",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "start-debug": "nodemon app.js",
    "test": "jest --coverage --coverageDirectory=output/coverage/jest --passWithNoTests",
    "eslint:fix": "eslint --fix \"**/*.*\""
  },
  "jest": {
    "testEnvironment": "node",
    "coverageReporters": [
      "text",
      "html",
      "cobertura"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ]
  },
  "jest-junit": {
    "outputDirectory": "output/coverage/junit",
    "outputName": "junit.xml",
    "usePathForSuiteName": "true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cfernando415/femmecubator-api-repo.git"
  },
  "author": "Femmecubator Engineering Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cfernando415/femmecubator-api-repo/issues"
  },
  "homepage": "https://github.com/cfernando415/femmecubator-api-repo#readme",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "eslint": "^7.11.0",
    "jest": "^26.5.3",
    "jest-junit": "^12.0.0",
    "nodemon": "^2.0.5",
    "npm-run-all": "^4.1.5"
  }
}
