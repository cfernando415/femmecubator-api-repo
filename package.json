{
  "name": "femmecubator-api-repo",
  "version": "1.0.0",
  "description": "Node.js backend for Femmecubator",
  "main": "app.js",
  "scripts": {
    "build": "webpack --mode production",
    "clean": "rm -rf build",
    "start": "node ./build/server.js",
    "start-debug": "nodemon ./server/app.js",
    "test": "jest --coverage --coverageDirectory=output/coverage/jest --passWithNoTests",
    "eslint:fix": "eslint --fix \"**/*.*\""
  },
  "jest": {
    "testEnvironment": "node",
    "coverageReporters": [
      "text",
      "html",
      "cobertura"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ]
  },
  "jest-junit": {
    "outputDirectory": "output/coverage/junit",
    "outputName": "junit.xml",
    "usePathForSuiteName": "true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cfernando415/femmecubator-api-repo.git"
  },
  "author": "Femmecubator Engineering Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cfernando415/femmecubator-api-repo/issues"
  },
  "homepage": "https://github.com/cfernando415/femmecubator-api-repo#readme",
  "dependencies": {
    "@babel/core": "^7.11.6",
    "babel-jest": "^26.5.2",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "connect-timeout": "^1.9.0",
    "copy-webpack-plugin": "^6.2.1",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "http-status-codes": "^2.1.4",
    "nodemon-webpack-plugin": "^4.3.2",
    "webpack": "^5.1.0",
    "webpack-cli": "^4.0.0"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "eslint": "^7.11.0",
    "jest": "^26.5.3",
    "jest-junit": "^12.0.0",
    "nodemon": "^2.0.5",
    "npm-run-all": "^4.1.5"
  }
}
